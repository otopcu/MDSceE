<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="ConceptualScenarioMetaModel.presentation.ConceptualScenarioMetaModelModelWizardID"
            name="%_UI_ConceptualScenarioMetaModelModelWizard_label"
            class="ConceptualScenarioMetaModel.presentation.ConceptualScenarioMetaModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ConceptualScenarioMetaModelModelFile.gif">
