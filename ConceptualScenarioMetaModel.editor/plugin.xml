   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="ConceptualScenarioMetaModel.presentation.ConceptualScenarioMetaModelEditorID"
            name="%_UI_ConceptualScenarioMetaModelEditor_label"
            icon="icons/full/obj16/ConceptualScenarioMetaModelModelFile.gif"
            extensions="conceptualscenariometamodel"
            class="ConceptualScenarioMetaModel.presentation.ConceptualScenarioMetaModelEditor"
            contributorClass="ConceptualScenarioMetaModel.presentation.ConceptualScenarioMetaModelActionBarContributor">
      </editor>
